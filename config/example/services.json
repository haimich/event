    "env": { "NODE_ENV": "example" }
   },{
    "name"        : "frontend",
    "script"      : "server.js",
    "args"        : ["--config=/home/event/event-internal/config/frontend/dev.yml", "--ports=/home/event/event-internal/config/ports/dev.yml"],
    "merge_logs"  : true,
    "cwd"         : "frontend/",
    "env": { "NODE_ENV": "example" }
   },{
    "name"        : "user-api",
    "script"      : "server.js",
    "args"        : ["--config=/home/event/event-internal/config/users-api/dev.yml", "--ports=/home/event/event-internal/config/ports/dev.yml"],
    "merge_logs"  : true,
    "cwd"         : "users-api/",
    "env": { "NODE_ENV": "example" }
   },{
    "name"        : "session-api",
    "script"      : "server.js",
