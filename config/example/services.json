    "env": { "NODE_ENV": "example" }
   }
 ],
  "deploy" : {
    "dev" : {
      "user" : "event",
      "host" : "myexamplehost",
      "ref"  : "origin/master",
      "repo" : "https://github.com/haimich/event.git",
      "path" : "/home/event/event",
      "post-deploy" : "npm install; pm2 startOrRestart config/example/services.json --env example"
